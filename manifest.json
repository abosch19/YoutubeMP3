{
  "manifest_version": 2,

  "name": "MusicDownloader",
  "description": "This extensions convert YouTube video to mp3",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Donwload!"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png" 
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
